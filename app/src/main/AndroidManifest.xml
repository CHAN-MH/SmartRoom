<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.tarc.smartroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartRoom">
        <activity android:name=".UnlockWhichDoor"></activity>
        <activity android:name=".Door" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayUnlock" />
        <activity android:name=".Unlock" />
        <activity android:name=".DisplayTime" />
        <activity android:name=".DisplayRoom" />
        <activity android:name=".Reservation" />
        <activity android:name=".Timer" />
    </application>

</manifest>